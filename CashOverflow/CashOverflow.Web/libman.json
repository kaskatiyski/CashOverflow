{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@5.8.2",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "moment.js@2.24.0",
      "destination": "wwwroot/lib/moment.js/"
    },
    {
      "library": "air-datepicker@2.2.3",
      "destination": "wwwroot/lib/air-datepicker/"
    },
    {
      "library": "Chart.js@2.8.0",
      "destination": "wwwroot/lib/Chart.js/"
    },
    {
      "provider": "jsdelivr",
      "library": "chartjs-plugin-datalabels@0.6.0",
      "destination": "wwwroot/lib/chartjs-plugin-datalabels/",
      "files": [
        "dist/chartjs-plugin-datalabels.js",
        "dist/chartjs-plugin-datalabels.min.js"
      ]
    },
    {
      "library": "chartjs-plugin-zoom@0.6.6",
      "destination": "wwwroot/lib/chartjs-plugin-zoom/"
    },
    {
      "library": "hammer.js@2.0.8",
      "destination": "wwwroot/lib/hammer.js/"
    }
  ]
}